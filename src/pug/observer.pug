-
	var domain = './'
	var imgSrc = './img/'
	var description = 'observer'
	var keywords = 'observer'
	var title = 'observer'
	var date = +new Date()

doctype html
html(lang='ja')
	head
		include include/_meta
		include include/_ogp
		include include/_title
		include include/_css
	body
		#observer(data-target-observer)
			ul.errors
			form
				label(for="title") title
				input#title.title(type="text")
				label(for="body") body
				input#body.body(type="text")

		include include/_js